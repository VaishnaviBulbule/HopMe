        #map {
            height: 400px;
            width: 100%;
            background-color: grey;
        }
    </style>
    <script>
        function initMap() {
            var center = { lat: -37.8023718, lng: 144.8991083 };
            var locations = [
                ['Asylum Seeker Resource Centre<br>\
           214-218 Nicholson St, Footscray VIC 3011 Australia <br>\
           <a href="https://goo.gl/maps/gozVHR1XvqtjPiqo9" target= "_blank">Get Directions</a>', -37.8023718, 144.8991083],
                ['Friends of Refugees<br>\
            1D Parsons Ave, Springvale VIC 3171 Australia<br>\
           <a href="https://goo.gl/maps/V9pqC8Nqwr2vK5b28"target= "_blank">Get Directions</a>', -37.9440058, 145.151487],
                ['RISE ABILITY RIGHTS<br>\
            Level 1/247 Flinders Ln, Melbourne VIC 3000 Australia<br>\
            <a href="https://goo.gl/maps/LPbfCFcDbSsbFVTz9"target= "_blank">Get Directions</a>', -37.8193632, 144.9571746],
                ['Southern Migrant and Refugee Centre<br>\
           39 Clow St, Dandenong VIC 3175 Australia<br>\
            <a href="https://goo.gl/maps/GxfMAu6rh1PnjNsJA" target= "_blank" >Get Directions</a>', -37.9846905, 145.2167528],
                ['Asylum Seeker Welcome Centre<br>\
            212 Sydney Rd, Brunswick VIC 3056 Australia<br>\
           <a href="https://goo.gl/maps/Ttb2upLpNMZS1M93A" target= "_blank">Get Directions</a>', -37.9968258, 145.1942233],
                ['Refugee & Immigration Legal Centre<br>\
                6/20 Otter St, Collingwood VIC 3066 Australia<br>\
           <a href="https://goo.gl/maps/t6vTJTR5abAXm4Pq6" target= "_blank">Get Directions</a>', -37.8008999, 144.9857652],
                ['Refugee Migrant Children Centre (RMCC)<br>\
                 21A Sun Cres, Sunshine VIC 3020 Australia<br>\
                <a href="https://goo.gl/maps/xt1Rs2FLSKu9YrkP7" target= "_blank">Get Directions</a>', -37.7885452, 144.8313795],
                ['Refuge of Hope<br>\
                15/20 Commercial Rd, Melbourne VIC 3004 Australia<br>\
                <a href="https://goo.gl/maps/noxSNYgBmLQsLgEy5" target= "_blank">Get Directions</a>', -37.8455762, 144.9844064],

                ['Immigration & Refugee Specialists PTY LTD<br>\
                 416-420 Collins St, Melbourne VIC 3000 Australia<br>\
                <a href="https://goo.gl/maps/GdxautBmYWZ87uxx6" target= "_blank">Get Directions</a>', -37.8189955, 144.9541445],
                ['Litsa Pavlou, Migration and Refugee Lawyer<br>\
                333 Collins St, Melbourne VIC 3000 Australia<br>\
                <a href="https://goo.gl/maps/xdiSuTgHXw7ha5iK8" target= "_blank">Get Directions</a>', -37.817355, 144.963304]
                           

            ];
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 9,
                center: center
            });
            var infowindow = new google.maps.InfoWindow({});
            var marker, count;
            for (count = 0; count < locations.length; count++) {
                marker = new google.maps.Marker({
                    position: new google.maps.LatLng(locations[count][1], locations[count][2]),
                    map: map,
                    title: locations[count][0]
                });
                google.maps.event.addListener(marker, 'click', (function (marker, count) {
                    return function () {
                        infowindow.setContent(locations[count][0]);
                        infowindow.open(map, marker);
                    }
                })(marker, count));
            }
        }
    </script>
 
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCYOHR9rYbH1I199DNFwfsFOutr64DE3SE&callback=initMap">
    </script>
</head>

<body>
